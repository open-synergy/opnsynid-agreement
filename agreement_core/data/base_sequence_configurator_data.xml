<openerp>
<data>
<!-- Training -->
<record id="base_sequence_agreement_agreement" model="base.sequence_configurator">
    <field name="model_id" ref="agreement_core.model_agreement_agreement"/>
    <field name="sequence_field_id" search="[('model_id.model','=','agreement.agreement'),('name','=','name')]"/>
    <field name="fallback_sequence_id" ref="agreement_core.sequence_agreement_agreement"/>
</record>

<record id="base_sequence_agreement_agreement_line" model="base.sequence_configurator_line">
    <field name="generator_id" ref="agreement_core.base_sequence_agreement_agreement"/>
    <field name="sequence" eval="1"/>
    <field name="sequence_computation_code">result = document.type_id.sequence_id</field>
</record>
</data>
</openerp>
